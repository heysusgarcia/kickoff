<ul class="backers-list">
  <% backers.each(function(backer) { %>
    <li>
      <div class="row">
        <div class="col-md-2">
            <img class="profile-photo" src="<%= backer.escape('profile')%>">
        </div>
        <div class="col-md-10">
          <p class="backer-name">
            <%= backer.escape("name")%>
            <span class="commenttime grey-dark"><%=new Date()%></span>
          </p>
          <p>Backed <%= backer.backedProjects.length %> projects!</p>
        </div>
      </div>
    </li>
  <% }) %>
</ul>
